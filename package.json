{
  "name": "proyecto",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check:contrast": "node scripts/contrast.mjs",
    "check:a11y": "npx @axe-core/cli http://localhost:4321",
    "check:legal": "node scripts/check-legal.mjs",
    "check:business": "node scripts/check-business.mjs",
    "check:chatbot": "node scripts/check-chatbot.mjs",
    "check:all": "npm run check:contrast && npm run check:a11y && npm run check:legal && npm run check:business && npm run check:chatbot",
    "lighthouse": "npx lighthouse http://localhost:4321 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "npx @lhci/cli autorun",
    "test:build": "npm run build && npm run preview",
    "test:deploy": "npm run build && npx serve dist -p 3000",
    "backup:content": "node scripts/backup-content.mjs",
    "backup:images": "node scripts/backup-images.mjs",
    "backup:all": "npm run backup:content && npm run backup:images",
    "update:deps": "npm update && npm audit fix",
    "clean": "rm -rf dist node_modules/.cache .astro",
    "fresh": "npm run clean && npm install && npm run build"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.0",
    "@astrojs/tailwind": "^6.0.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "astro": "^5.14.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.545.0",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.7.0"
  }
}
